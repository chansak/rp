}

@section Scripts {
    @Scripts.Render("~/plugins/iCheck")
    @Scripts.Render("~/plugins/dataPicker")
    @Scripts.Render("~/plugins/sweetAlert")
    @Scripts.Render("~/plugins/dropZone")
    @Scripts.Render("~/plugins/jasnyBootstrap")
    @Scripts.Render("~/plugins/codeEditor")
    @Scripts.Render("~/plugins/dataPicker")
    @Scripts.Render("~/plugins/typehead")
    @Scripts.Render("~/plugins/currency")
    @Scripts.Render("~/plugins/fullCalendar")
    @Scripts.Render("~/plugins/toastr")

    <script type="text/javascript">
        $(document).ready(function () {
            $(".next").show();
            $(".prev").hide();
            // Handling data-toggle manually
            $('.nav-tabs a').click(function () {
                $(this).tab('show');
            });
            // The on tab shown event
            $('.nav-tabs a').on('shown.bs.tab', function (e) {
                $(".next").show();
                $(".prev").show();
                var current_tab = e.target;
                var previous_tab = e.relatedTarget;
                var tabName = ($('.nav-tabs > .active').find('a').attr('href'));
                console.log(tabName);
                if (tabName == '#tab-1') {
                    $(".prev").hide();
                    $(".next").show();
                }
                if (tabName == '#tab-5') {
                    $(".prev").show();
                    $(".next").hide();
                }
            });
            $('.i-checks').iCheck({
                checkboxClass: 'icheckbox_square-green',
                radioClass: 'iradio_square-green',
            });
            $(".date").datepicker({
                startDate: new Date(),
                todayHighlight: true,
                format: "dd/mm/yyyy",
                autoclose: true,
            });
            $(".date").datepicker('setDate', new Date());
            $(".iCheck-helper").click(function () {
                var id = $(this).prev().prop("id");
                switch (id) {
                    case "print-optional1":
                        {
                            $("#print-option1-section").show();
                            $("#print-option2-section").hide();
                            $("#print-option3-section").hide();
                            $("#print-pattern").val($("#print-pattern option:first").val());
                            $("#print-patternImage").html("");
                            break;
                        }
                    case "print-optional2":
                        {
                            $("#print-option1-section").hide();
                            $("#print-option2-section").show();
                            $("#print-option3-section").hide();
                            $("#print-patternImage").html("");
                            break;
                        }
                    case "print-optional3":
                        {
                            $("#print-option1-section").hide();
                            $("#print-option2-section").hide();
                            $("#print-option3-section").show();
                            $("#print-patternImage").html("");
                            break;
                        }
                    case "screen-optional1":
                        {
                            $("#screen-option1-section").show();
                            $("#screen-option2-section").hide();
                            $("#screen-option3-section").hide();
                            $("#screen-pattern").val($("#screen-pattern option:first").val());
                            $("#screen-patternImage").html("");
                            break;
                        }
                    case "screen-optional2":
                        {
                            $("#screen-option1-section").hide();
                            $("#screen-option2-section").show();
                            $("#screen-option3-section").hide();
                            $("#screen-patternImage").html("");
                            break;
                        }
                    case "screen-optional3":
                        {
                            $("#screen-option1-section").hide();
                            $("#screen-option2-section").hide();
                            $("#screen-option3-section").show();
                            $("#screen-patternImage").html("");
                            break;
                        }
                    case "sew-optional1":
                        {
                            $("#sew-option1-section").show();
                            $("#sew-option2-section").hide();
                            $("#sew-option3-section").hide();
                            $("#sew-pattern").val($("#sew-pattern option:first").val());
                            $("#sew-patternImage").html("");
                            break;
                        }
                    case "sew-optional2":
                        {
                            $("#sew-option1-section").hide();
                            $("#sew-option2-section").show();
                            $("#sew-option3-section").hide();
                            $("#sew-patternImage").html("");
                            break;
                        }
                    case "sew-optional3":
                        {
                            $("#sew-option1-section").hide();
                            $("#sew-option2-section").hide();
                            $("#sew-option3-section").show();
                            $("#sew-patternImage").html("");
                            break;
                        }
                }
            });
            $(".btnSave").click(function () {
                documentCreator.SaveDocument(function () {
                    swal({
                        title: "บันทึกเรียบร้อย!",
                        text: "กดปุ่ม OK เพื่อปิด!",
                        icon: "success"
                    }).then((value) => {
                        utilities.RedirectToAction("/Sale/Document/Index");
                    });
                });
            });
            $(".btnSaveDraft").click(function () {
                documentCreator.SaveDraftDocument(function () {
                    swal({
                        title: "บันทึกเรียบร้อย!",
                        text: "กดปุ่ม OK เพื่อปิด!",
                        icon: "success"
                    }).then((value) => {
                        utilities.RedirectToAction("/Sale/Document/Index");
                    });
                });
            });
            $("#btnAddNewProduct").click(function () {
                var cid = $("#auto_customerId").val();
                if (cid == "") {
                    toastr.info("กรุณาเลือกลูกค้าก่อนทำรายการเพิ่มสินค้า", 'Infomration');
                } else {
                    $("#addProductModal").modal({ backdrop: "static" });
                    productCreator.init(cid);
                }
            });
            $("#btnAddNewItem").click(function () {
                $("#addProductModal").modal("hide");
                productCreator.AddNewItem();
                documentCreator.RenderProducts();
            });
            $("#materials").change(function () {
                productCreator.MaterialStockCheck();
            });
            $("#priceValidityDays").change(function () {
                documentCreator.ExpirationCalculation();
            });
            $('.next').click(function () {
                $('.nav-tabs > .active').next('li').find('a').trigger('click');
                var tabName = ($('.nav-tabs > .active').find('a').attr('href'));
                if (tabName == '#tab-5') {
                    $(".next").hide();
                }
                $(".prev").show();
            });
            $('.prev').click(function () {
                $('.nav-tabs > .active').prev('li').find('a').trigger('click');
                var tabName = ($('.nav-tabs > .active').find('a').attr('href'));
                if (tabName == '#tab-1') {
                    $(".prev").hide();
                }
                $(".next").show();
            });

            documentCreator.init();
        });
    </script>
}
